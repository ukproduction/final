<!-- About Area Start -->
<section class="support-company-area fix pt-10">
    <div class="support-wrapper align-items-end">
        <div class="left-content">
            <!-- section tittle -->
            <div class="section-tittle section-tittle2 mb-55">
                <div class="front-text">
                    <h2 class="">Who we are</h2>
                </div>

            </div>
            <div class="support-caption">
                <p class="pera-top">We are providing construction platform specifically for house related services/construction materials to user, where list of construction relevant material will be available.<br /><br /> @Html.ActionLink("Read More >>", "About", "Home", new { @class = "btndefault" })</p>


            </div>
        </div>
        <div class="right-content">
            <!-- img -->
            <div class="right-img">

                <img src="~/content/image/gallery/safe_in.png" />
            </div>

        </div>
    </div>
</section>
<br />
<div class="container">
    <div class="section-tittle mb-55">
        <div class="front-text">
            <h2 class="">Our Projects</h2>
        </div>
        <span class="back-text">Services</span>
    </div>
    <div class="row">
        @foreach (var item in Model)
        {
        <div class="col-lg-3" style="padding:10px;text-align:center;border-color:lightgray;border:1px solid lightgray;margin:0px;">

            <img src="@Url.Content(item.Image)" style="width:280px;height:250px;" />
            <h5></h5>
            <h5></h5>
            <h5 style="align-content:center;"> @Html.DisplayFor(modelItem => item.Name)</h5>
            <br />
            @Html.ActionLink("Read More", "Project_Details","Account", new { @class = "btndefault" }) 

            @*<h5 style="align-content:center;">Rating: @Html.DisplayFor(modelItem => item.Rating)</h5>*@

        </div>

        }
    </div>

    @Html.PagedListPager(Model, i => Url.Action("Index", "Home", new { i, search = Request.QueryString["search"] }))
</div>

