                    </div>
                    <div class="service-cap">
                        <h4><a href="services_details.html">Paintaing labor Service</a></h4>
                        <button style="">@Html.ActionLink("Read More", "Services", new { @class = "btndefault" }) <i class="ti-plus"></i></button>
                    </div>
                    <div class="service-icon">
                        <img src="~/content/image/icon/services_icon1.png" alt="content folder">
                    </div>
                </div>
            </div>
            
        </div>
