@model IPagedList<Hombay.tbl_items>
@using PagedList.Mvc;
@using PagedList;
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/_LayoutPage2.cshtml";
}


@if (Session["UserName"] != null)
{
    @Session["UserName"].ToString()
}
<style>
    .box-title {
        color: white;
      
    }
</style>
<head>
    <link href="~/content/PagedList.css" rel="stylesheet" />
</head>

<div class="page-wrapper">
    <!-- ============================================================== -->
    <!-- Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <div class="page-breadcrumb bg-white">
        <div class="row align-items-center">
            <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                <h4 class="page-title text-uppercase font-medium font-14">Dashboard</h4>
            </div>
            <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
                <div class="d-md-flex">
                   
                </div>
            </div>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- ============================================================== -->
    <!-- End Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Container fluid  -->
    <!-- ============================================================== -->
    <div class="container-fluid">
        <!-- ============================================================== -->
        <!-- Three charts -->
        <!-- ============================================================== -->
        <div class="row justify-content-center">
            <div class="col-lg-4 col-sm-6 col-xs-12">
                <div class="white-box analytics-info" style="background-color:dodgerblue;">
                    <h3 class="box-title">My Item</h3>
                    <ul class="list-inline two-part d-flex align-items-center mb-0">
                        <li>
                            <div id="sparklinedashs">
                                <img src="~/images/lastmmax.png" width="50" height="40" />

                            </div>
                        </li>
                        <li class="ml-auto"><span class="counter text-white">@Session["selleritem"].ToString()</span></li>
                    </ul>
                </div>
            </div>
            <div class="col-lg-4 col-sm-6 col-xs-12">
                <div class="white-box analytics-info" style="background-color:mediumseagreen;">
                    <h3 class="box-title">My Item Price</h3>
                    <ul class="list-inline two-part d-flex align-items-center mb-0">
                        <li>
                            <div id="sparklinedash2s">
                                <img src="~/images/089-task.png" width="50" height="40" />
                            </div>
                        </li>
                        <li class="ml-auto"><span class="counter text-white">
                       @if (!string.IsNullOrEmpty(Session["itemprice"] as string))

                       {
                           Session["itemprice"].ToString();
                       }

                          </span></li>
                    </ul>
                </div>
            </div>
            <div class="col-lg-4 col-sm-6 col-xs-12">
                <div class="white-box analytics-info" style="background-color:darkmagenta;">
                    <h3 class="box-title">My Compalints</h3>
                    <ul class="list-inline two-part d-flex align-items-center mb-0">
                        <li>
                            <div id="sparklinedash3s">
                                <img src="~/images/033-Graphics design.png" width="50" height="40" />
                            </div>
                        </li>
                        <li class="ml-auto">
                            <span class="counter text-white">@Session["mycomplaint"].ToString()</span>
                        </li>
                    </ul>
                </div>
            </div>




            <div class="col-lg-4 col-sm-6 col-xs-12">
                <div class="white-box analytics-info" style="background-color:brown;">
                    <h3 class="box-title">My Review</h3>
                    <ul class="list-inline two-part d-flex align-items-center mb-0">
                        <li>
                            <div id="sparklinedash4s">
                                <img src="~/images/alltask.png" width="50" height="40" />
                            </div>
                        </li>
                        <li class="ml-auto"><span class="counter text-white">@Session["myreview"].ToString()</span></li>
                    </ul>
                </div>
            </div>
            <div class="col-lg-4 col-sm-6 col-xs-12">
                <div class="white-box analytics-info" style="background-color:saddlebrown;">
                    <h3 class="box-title">My Query</h3>
                    <ul class="list-inline two-part d-flex align-items-center mb-0">
                        <li>
                            <div id="sparklinedashs">
                                <img src="~/images/todayproject.png" width="50" height="40" />
                            </div>
                        </li>
                        <li class="ml-auto"><span class="counter text-white">@Session["myquery"].ToString()</span></li>
                    </ul>
                </div>
            </div>
            <div class="col-lg-4 col-sm-6 col-xs-12">
                <div class="white-box analytics-info" style="background-color:deeppink;">
                    <h3 class="box-title">My Item Activity</h3>
                    <ul class="list-inline two-part d-flex align-items-center mb-0">
                        <li>
                            <div id="sparklinedash3s">
                                <img src="~/images/027-magnifying glass.png" width="50" height="40" />

                            </div>
                        </li>
                        <li class="ml-auto">
                            <span class="counter text-white">@Session["selleritem"].ToString()</span>
                        </li>
                    </ul>
                </div>
            </div>

        </div>

        <!-- ============================================================== -->
        <!-- RECENT SALES -->
        <!-- ============================================================== -->



        <div class="row">
            <div class="col-md-12 col-lg-12 col-sm-12">
                <div class="white-box">

                    <table class="table">
                        <tr>
                            <th>

                                @Html.DisplayNameFor(model => model.First().Name)
                            </th>

                            <th>
                                @Html.DisplayNameFor(model => model.First().Mobile)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.First().Category)
                            </th>


                            <th>
                                @Html.DisplayNameFor(model => model.First().Date)
                            </th>



                            <th>
                                @Html.DisplayNameFor(model => model.First().Image)
                            </th>

                        </tr>

                        @foreach (var item in Model)
                        {
                    <tr>
                        @if (Model != null)
                        {

                            <td>
                                @Html.DisplayFor(modelItem => item.Name)
                            </td>
                        }
                            @if (Model != null)
                            {

                                <td>
                                    @Html.DisplayFor(modelItem => item.Mobile)
                                </td>
                            }
                            @if (Model != null)
                            {

                                <td>
                                    @Html.DisplayFor(modelItem => item.Category)
                                </td>
                            }

                            @if (Model != null)
                            {

                                <td>
                                    @Html.DisplayFor(modelItem => item.Date)
                                </td>

                            }
                            @if (Model != null)
                            {


                                <td>
                                    <img src="@Url.Content(item.Image)" style="width:50px;height:50px;" />
                                </td>

                            }
                        </tr>
                        }

                    </table>

                    @Html.PagedListPager(Model, i => Url.Action("Index", "Seller", new { i, search = Request.QueryString["search"] }))




                </div>
            </div>
        </div>




        <!-- ============================================================== -->

    </div>
</div>
<!-- ============================================================== -->
<!-- End Page wrapper  -->
<!-- ============================================================== -->
